{
    "name": "groton-school/slim-gae-shim",
    "description": "Shim for Slim Skelton to run on Google App Engine",
    "authors": [
        {
            "name": "Seth Battis",
            "email": "sbattis@groton.org"
        }
    ],
    "license": "GPL-3.0",
    "require": {
        "google/cloud-logging": "^1.32",
        "php-di/php-di": "^6.0",
        "psr/http-message": "^2.0",
        "slim/slim": "^4.14"
    },
    "autoload": {
        "psr-4": {
            "GrotonSchool\\Slim\\GAE\\": "src/"
        }
    },
    "scripts": {
        "release": "conventional-changelog --commit"
    },
    "require-dev": {
        "franzl/studio": "^0.16.0",
        "marcocesarato/php-conventional-changelog": "^1.17"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "franzl/studio": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "version": "1.0.0"
}